<div>
    <table>
        <tr>
            <th>Name</th>
            <th>Surname</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Gender</th>
        </tr>
       {% for item in data %}
        <tr>
        <td>{{ item.name }}</td>
        <td>{{ item.surname }}</td>
        <td>{{ item.email }}</td>
        <td>{{ item.phone }}</td>
        {% if item.gender == 1 %}
        <td>Male</td>
        {% else %}
        <td>Female</td>
        {% endif %}
        </tr>
        {% endfor %}

    </table>
</div>